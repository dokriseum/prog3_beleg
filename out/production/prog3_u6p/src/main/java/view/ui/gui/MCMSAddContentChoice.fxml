<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="180.0" prefWidth="320.0" stylesheets="@Style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ui.gui.HubControllerAddContentChoice">
    <children>
        <Pane stylesheets="@Style.css" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <HBox prefHeight="180.0" prefWidth="320.0" spacing="5.0">
                    <children>
                        <VBox spacing="5.0" HBox.hgrow="ALWAYS">
                            <children>
                                <Label prefHeight="7.0" prefWidth="164.0" text="Media Type:" />
                                <Button mnemonicParsing="false" onAction="#clickButtonAddInteractiveVideo" text="Interactive Video" />
                                <Button mnemonicParsing="false" onAction="#clickButtonAddLicensedAudioVideo" text="Licensed Audio Video" />
                            </children>
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                        </VBox>
                        <VBox alignment="BOTTOM_RIGHT" spacing="5.0" HBox.hgrow="ALWAYS">
                            <children>
                                <Button fx:id="buttonCancel" mnemonicParsing="false" onAction="#clickButtonCancel" text="Cancel" />
                            </children>
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                     <padding>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                     </padding>
                        </VBox>
                    </children>
               <padding>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </padding>
                </HBox>


            </children>
        </Pane>
    </children>
</AnchorPane>
